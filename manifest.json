{
  "manifest_version": 2,

  "name": "Fluent",
  "description": "A gorgeous extension for customizing your New Tab page.",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "html/fluent.html"
  },

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "Fluent Settings"
  },

  "permissions": [
    "activeTab",
    "storage",
    "bookmarks",
    "chrome://favicon/"
  ]
}
